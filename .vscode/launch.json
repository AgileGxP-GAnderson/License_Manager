{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: Debug Server",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "skipFiles": ["<node_internals>/**"],
      "sourceMapPathOverrides": {
        "webpack://_N_E/../*": "${workspaceFolder}/*",
        "webpack:///*": "*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack:///src/*": "${workspaceFolder}/*"
      },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}